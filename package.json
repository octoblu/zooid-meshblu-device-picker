{
  "name": "zooid-meshblu-device-picker",
  "version": "1.0.1",
  "description": "MeshbluDevicePicker",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint src stories",
    "lint:js:fix": "eslint src stories --fix",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:css:fix": "stylefmt ./src/**/*.css",
    "build": "webpack",
    "test": "nyc --require babel-core/register --require babel-polyfill mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-meshblu-device-picker.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-meshblu-device-picker/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-meshblu-device-picker#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js"
    ]
  },
  "devDependencies": {
    "@kadira/storybook": "^1.28.1",
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.4.2",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^4.0.1",
    "enzyme": "^2.3.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-import": "^1.6.0",
    "eslint-plugin-jsx-a11y": "^1.0.3",
    "eslint-plugin-react": "^5.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^2.0.0",
    "jsdom": "^9.1.0",
    "mocha": "^2.4.5",
    "mocha-multi": "^0.9.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^6.4.4",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.0.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "stylefmt": "^3.5.0",
    "stylelint": "^6.6.0",
    "stylelint-config-standard": "^9.0.0",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "classnames": "^2.2.5",
    "lodash.noop": "^3.0.1",
    "react": "^15.0.1",
    "react-select": "^1.0.0-beta13"
  }
}
